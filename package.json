{
  "name": "edotofamily-api",
  "version": "6.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "clean": "rimraf node_modules",
    "build": "nest build && cpx \"src/categories/db/edotofamily_categories_full.json\" dist/categories/db/ && cpx \"src/database/sql/**/*.sql\" dist/src/database/sql/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node --max-old-space-size=1024 dist/src/main.js",
    "start:prod": "node --max-old-space-size=1024 dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.840.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "1.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/typeorm": "^11.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.2.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^2.7.0",
    "firebase-admin": "^13.4.0",
    "fuse.js": "^6.6.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.14.1",
    "nestjs-stripe": "^1.0.0",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "paypal-rest-sdk": "^1.8.1",
    "pdfkit": "^0.17.2",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "sib-api-v3-sdk": "^8.5.0",
    "stripe": "^11.2.0",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@types/aws-lambda": "^8.10.152",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.7",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.17.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "cpx": "^1.5.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}